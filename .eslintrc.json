{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "new-cap": 0,
        "max-len": ["error", 300],
        "object-curly-spacing": 0,
        "space-before-function-paren": 0,
        "linebreak-style": ["error", "windows"]
    }
}
